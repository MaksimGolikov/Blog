
@model IEnumerable<Blog_.Models.DataBase.Topic>

    @{
        Layout = "~/Views/AdministoratorPage.cshtml";
    }


    <div id="main">
        <div id="picture">
            <h4> Administrator part</h4>
        </div>
        <div id="button">
            <a href="~/Admin/Create">
                <h4> Add topic</h4>
            </a>
            <a href="~/Admin/Edit">
                <h4> Change topic </h4>
            </a>
            <a href="~/Admin/Delete">
                <h4> Delete topic </h4>
            </a>
        </div>
    </div>

  

    <div class="docs-content">                   
            @foreach (var item in Model)
            {
                <a href="~/Admin/Del/@item.Id">@item.NameTopic</a> <br />                 
            }
    </div>
